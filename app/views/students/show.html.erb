<p id="notice"><%= notice %></p>

<p>
  <b>Student:</b>
  <%= @student.student_id %>
</p>

<p>
  <b>Name:</b>
  <%= @student.name %>
</p>

<p>
  <b>Nickname:</b>
  <%= @student.nickname %>
</p>

<p>
  <b>Project (40%):</b>
  <%= @student.project %>%
</p>

<p>
  <b>Final (40%):</b>
  <%= @student.final %>%
</p>

<p>
  <b>Lab (10%):</b>
  <%= @student.lab %>%
</p>

<p>
  <b>Class (10%):</b>
  <%= @student.classdiscussion %>%
</p>

<p>
  <b>Total:</b>
  <%= @student.total %>%
</p>


<div class="btn btn-info"><%= link_to 'Edit', edit_student_path(@student) %></div>
<div class="btn btn-info"><%= link_to 'Back', students_path %></div>
